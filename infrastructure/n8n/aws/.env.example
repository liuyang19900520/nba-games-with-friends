# n8n Configuration
# Copy this file to .env and fill in the values

# Your server's public IP (use nip.io format for auto-SSL)
N8N_HOST=YOUR_PUBLIC_IP.nip.io

# n8n Encryption Key (CRITICAL!)
# Get from existing n8n: cat /var/lib/docker/volumes/n8n_n8n_data/_data/config
# Must use the same key, otherwise saved credentials cannot be decrypted
N8N_ENCRYPTION_KEY=your-encryption-key-here

# Timezone
TIMEZONE=Asia/Tokyo
